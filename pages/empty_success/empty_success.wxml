<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" color="#3E85FF" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">空载调度创建并下发成功</view>
            <view class=" title">调度单号：{{num}}</view>
            <view class=" title">线路名称：{{routeName}}</view>
            <view class=" title">车牌号码：{{truckLicense}}</view>
            <view class=" title">司机姓名：{{driverName}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area" style='margin-top:40px'>
              <button class="weui-btn createdDispatch" type="primary" bindtap="back">返回</button>
              <button class="weui-btn createdDispatch1" type="primary" bindtap="call">联系司机</button>
            </view>
        </view>
    </view>
</view>