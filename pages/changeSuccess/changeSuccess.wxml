<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" color="#3E85FF" size="93"></icon>
        </view>
        <view class="weui-msg__text-area" style="padding-left:30px">
            <view class="weui-msg__title" style="margin-bottom:20px">更新调度单状态成功</view>
            <view class="title">调度单号：{{wayBillNum}}</view>
            <view class="title">线路名称：{{routeName}}</view>
            <view class="title"><text>调度状态：由</text><text class="status">{{status}}</text><text>变更为</text><text class="status">{{statusLabel}}</text> </view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area" style='margin-top:80px'>
                <button class="weui-btn createdDispatch" type="primary" bindtap="back">返回</button>
            </view>
        </view>
    </view>
</view>