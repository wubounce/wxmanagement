<view class="page">
    <view class="weui-msg">
        <view class="weui-msg__icon-area">
            <icon type="success" color="#3E85FF" size="93"></icon>
        </view>
        <view class="weui-msg__text-area">
            <view class="weui-msg__title">空载调度创建成功</view>
            <view class="weui-msg__desc">调度单号：{{num}}</view>
            <view class="weui-msg__desc">线路名称：{{routeName}}</view>
            <view class="weui-msg__desc">车牌号：{{truckLicense}}</view>
            <view class="weui-msg__desc">司机姓名：{{driverName}}</view>
        </view>
        <view class="weui-msg__opr-area">
            <view class="weui-btn-area">
                <button class="weui-btn" type="primary">确定</button>
            </view>
        </view>
    </view>
</view>