.header {
  width: 100%;
  background: #308bef;
  background-image: -webkit-linear-gradient(top, #308bef, #338AF1);
  background-image: -moz-linear-gradient(top, #308bef, #338AF1);
  background-image: -ms-linear-gradient(top, #308bef, #338AF1);
  background-image: -o-linear-gradient(top, #308bef, #338AF1);
  background-image: linear-gradient(to bottom, #308bef, #338AF1);
  padding: 30px 0;
  display: flex;
  justify-content: center;
  color: #fff;
  text-align:center;
}
.mileage {
  padding:0 30rpx;
}
.title {
  display:flex;
  color: #333;
}
.finish {
  text-align:right;
  color: #f00;
}
.createdDispatch {
  margin-top:100px;
  background:#3E85FF !important;
  border-radius:25px;
  width:90%;
}
.time {
  font-size:12px;
}